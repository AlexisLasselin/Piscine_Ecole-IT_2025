a = 3
b = 5

# Test simple : multiplication avant addition
c = a + b * 2
print(c)   # attendu : 13 (3 + (5*2))

# Test avec parenthèses : addition avant multiplication
d = (a + b) * 2
print(d)   # attendu : 16 ((3+5)*2)

# Division et multiplication
e = b * 4 / 2
print(e)   # attendu : 10 (20 / 2)

# Multiplication et soustraction
f = 10 - a * 2
print(f)   # attendu : 4 (10 - (3*2))

# Division avant addition
g = 10 + b / 5
print(g)   # attendu : 11 (10 + (5/5))

# Parenthèses qui inversent la priorité
h = (10 + b) / 5
print(h)   # attendu : 3 ((10+5)/5)
